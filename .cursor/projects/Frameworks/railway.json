{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS",
    "watchPatterns": [
      ".cursor/projects/Frameworks/**"
    ]
  },
  "deploy": {
    "startCommand": "cd .cursor/projects/Frameworks && node server.js",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
